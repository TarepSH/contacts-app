<p id="notice"><%= notice %></p>

<h1>Contacts Infos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>email</th>
      <th>Phone</th>
      <th>Address</th>
      <th>Compay</th>
      <th>Birthday</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts_infos.each do |contacts_info| %>
      <tr>
        <td><%= contacts_info.name %></td>
        <td><%= contacts_info.email %></td>
        <td><%= contacts_info.phone %></td>
        <td><%= contacts_info.address %></td>
        <td><%= contacts_info.compay %></td>
        <td><%= contacts_info.birthday %></td>
        <td><%= link_to 'Show', contacts_info %></td>
        <td><%= link_to 'Edit', edit_contacts_info_path(contacts_info) %></td>
        <td><%= link_to 'Destroy', contacts_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contacts Info', new_contacts_info_path %>
